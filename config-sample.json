{
  "accessories": [
    {
      "accessory": "aircon-ir-remote",
      "name": "Air Conditioner",
      "minSetpoint": 16,
      "defaultSetpoint": 20,
      "maxSetpoint": 30,
      "ir": {
        "name": "lirc_device_name"
      },
      "temp": {
        "command": "cat /sys/bus/w1/devices/28-01131650ab0a/w1_slave |grep t= | cut -d '=' -f 2",
        "multiple": 1000
      }
    }
  ]
}
